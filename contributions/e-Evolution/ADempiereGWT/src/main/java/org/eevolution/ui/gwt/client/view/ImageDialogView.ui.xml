<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:ad="urn:import:org.eevolution.ui.gwt.client.widget"
	xmlns:adc="urn:import:org.eevolution.ui.gwt.client.component">
	<ui:style>
		/* Add CSS here. See the GWT docs on UI Binder for more details */
	</ui:style>
	<g:VerticalPanel>
		<g:cell>
			<g:HorizontalPanel width ='100%'>
				<g:cell>
					<ad:Label ui:field='fileLabel'/>
				</g:cell>
				<g:cell>
					<g:FileUpload ui:field='fileButton'/>
				</g:cell>
			</g:HorizontalPanel>
		</g:cell>
		<g:cell horizontalAlignment='ALIGN_CENTER' width='50%'>
			<g:FormPanel ui:field ='fp'>
				<g:Image ui:field='image'/>
			</g:FormPanel>
		</g:cell>
		<g:cell>
			<adc:ConfirmPanel ui:field='confirmPanel'
				withCancelButton='true' withHelpButton='false' withRefreshButton='false'
				withResetButton='false' withCustomizeButton='false'
				withHistoryButton='false' withZoomButton='false' />
		</g:cell>
	</g:VerticalPanel>
</ui:UiBinder> 