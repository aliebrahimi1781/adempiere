<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd">

        <!-- Scans for application @Components to deploy -->
        <context:component-scan base-package="org.eevolution.domain" />
        
        <import resource="ADManager-mvc.xml" />
        <import resource="ADManager-infrastructure.xml" />      
        <import resource="ADManager-webflow.xml" />
        <import resource="ADManager-security.xml" />
        
        <bean id="myAspect" class="org.eevolution.service.Introductions"/>
        <bean id="ADTreeService" class="org.eevolution.service.impl.ADTreeServiceImpl"/>

</beans>